{
  "parser": "babel-eslint",
  "extends": [
    "prettier",
    "prettier/standard",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  }
}
